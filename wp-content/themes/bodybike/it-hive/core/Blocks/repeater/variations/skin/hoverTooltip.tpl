<div class="row repeater-container" data-control-type="repeaterVariationsToolTip" data-limit="{{limit}}" id="{{id}}" data-tpl="{{templateID}}" data-name="{{name}}" >
    {{label}}
    <div class="repeater-inner" {{sortable}}>
        {{children}}
    </div>
    <select name="" class="variations" data-control-id="{{id}}" ></select>
    <a class="button repeater-add-btn" data-control-id="{{id}}" href="#" >{{addText}}</a>
</div>