<div class="row repeater-container" data-control-type="repeater" id="{{id}}" data-limit="{{limit}}" data-tpl="#{{templateID}}" data-name="{{name}}" >
    {{label}}
    <div class="repeater-inner" {{sortable}}>
        {{children}}
    </div>
    <a class="button repeater-add-btn" data-control-id="{{id}}" href="#" >{{addText}}</a>
</div>