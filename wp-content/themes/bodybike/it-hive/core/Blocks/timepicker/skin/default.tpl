<div class="row item" data-control-type="timepicker" data-time-format="{{timeFormat}}" data-step="{{step}}"
     data-start="{{start}}" data-end="{{end}}">
    <label for="{{id}}">{{label}}</label>
    <input class="time-picker ui-timepicker-input" type="text" id="{{id}}" name="{{name}}" value="{{value}}"
           autocomplete="off">
</div>