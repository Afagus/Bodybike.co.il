<div class="row attachment item">
    <label for="#{{id}}">{{label}}</label>
    <input class="widefat" type="text" id="{{id}}" name="{{name}}" value="{{value}}" placeholder="{{placeholder}}" readonly />
    <div class="btn-controls">
        <a href="#" class="upload-video">{{textUpload}}</a>
        <a href="#" class="delete-video">{{textRemove}}</a>
    </div>
</div>